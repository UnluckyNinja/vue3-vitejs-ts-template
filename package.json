{
  "name": "vue3-vitejs-ts-template",
  "version": "0.0.0",
  "description": "Vue 3 + Typescript + Vite with curated libs, based on `yarn create vite`",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@vueuse/core": "^6.7.3",
    "vue": "^3.2.20"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.20.0",
    "typescript": "~4.4.4",
    "vite": "^2.6.10",
    "vite-plugin-windicss": "^1.4.12",
    "vue-tsc": "^0.28.7",
    "windicss": "^3.1.9"
  },
  "keywords": [
    "vite",
    "vue",
    "vitejs",
    "vue3",
    "typescript"
  ]
}
